{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["../MIFURO/next-env.d.ts", "**/*.ts", "**/*.tsx", "../MIFURO/pages/index.jsx", "../MIFURO/components/SongsList/SongsList.jsx"],
  "exclude": ["../MIFURO/node_modules"]
}
